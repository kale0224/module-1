#define _crt_secure_no_warnings
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define MAXLEN 100
#define MAX 10

struct list
{
    char mana[MAXLEN];
    char comp[MAXLEN];
    char name[MAXLEN];
    char mail[MAXLEN];
    char grad[MAXLEN];
    char majo[MAXLEN];
};

int main() {

    FILE* p_file = NULL;
    char str[MAXLEN];
    char* ptr = str;
            
    if (0 == fopen_s(&p_file, ".\\list.csv", "rt")) {
     
        while (ptr != NULL) {

            char* p_token, * p_next_token;  
                        
            if (NULL != fgets(str, MAXLEN, p_file))
            {                
                printf("%s\n", str);

                p_token = strtok_s(str, ",", &p_next_token);
                
                int i;

                if (str[i] == "조장"+)
                {
                    strcpy(L[i].mana, str[i]);
                    
                    i++;
                }
                else
                for (int i = 1; p_token != NULL; i++)
                {
                    //printf("%s\t", p_token);

                    p_token = strtok_s(NULL, ",", &p_next_token);
                }

            } 
            

            
        }

        fclose(p_file);
    }   

    return 0;
}

